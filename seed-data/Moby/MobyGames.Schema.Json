{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MobyGames Catalog Schema",
  "description": "A schema to validate the MobyGames JSON catalog format.",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "developers": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "genres": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "id": {
        "type": "integer"
      },
      "moby_score": {
        "type": ["number", "null"]
      },
      "moby_url": {
        "type": "string",
        "format": "uri"
      },
      "platforms": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "publishers": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "release_date": {
        "type": "string"
      },
      "title": {
        "type": "string"
      }
    },
    "required": [
      "developers",
      "genres",
      "id",
      "moby_score",
      "moby_url",
      "platforms",
      "publishers",
      "release_date",
      "title"
    ]
  }
}