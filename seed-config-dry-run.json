{
  "name": "Dry Run Test",
  "description": "Test configuration without actually importing data",
  "database_path": "database/RomCurator.db",
  "imports": [
    {
      "name": "No-Intro Dry Run",
      "importer": "nointro",
      "file_path": "seed-data/Samples/FFExample-NOINTRO.dat",
      "platform_name": "Super Nintendo Entertainment System"
    },
    {
      "name": "TOSEC Dry Run",
      "importer": "tosec", 
      "file_path": "seed-data/Samples/FFExample-TOSEC.dat",
      "platform_name": "Nintendo Entertainment System"
    },
    {
      "name": "MobyGames Dry Run",
      "importer": "mobygames",
      "file_path": "seed-data/Moby/Console/MobyGames-SNES-Catalog.json",
      "platform_name": "Super Nintendo Entertainment System"
    }
  ],
  "options": {
    "clear_existing_data": false,
    "verbose_logging": true,
    "dry_run": true
  }
}
